package Chapter4;
import java.util.Scanner;
class Phone {
	private String name, tel;
	public Phone(String name, String tel) {
		this.name = name;
		this.tel = tel;
	}
}

class PhoneBook {
	private Phone[] phone;
	private Scanner sc = new Scanner(System.in);
	int num = sc.nextInt();
	phone = new Phone[num];
	
	
	
}


